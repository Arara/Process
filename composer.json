{
    "name": "arara/process",
    "type": "library",
    "description": "Provides a better API to work with processes on Unix-like systems",
    "keywords": ["fork", "daemon", "multitasking", "pcntl", "posix", "process", "spawn", "task", "command"],
    "homepage": "https://github.com/Arara/Process",
    "license" : "MIT",
    "authors" : [
        {
            "name": "Henrique Moody",
            "email": "henriquemoody@gmail.com"
        }
    ],
    "require": {
        "ext-pcntl": "*",
        "ext-posix": "*",
        "php": ">=5.4.0"
    },
    "require-dev": {
        "ext-uopz": "*",
        "phpmd/phpmd" : "~2.1.3",
        "phpunit/phpunit": "~4.3.5",
        "squizlabs/php_codesniffer": "~1.5.5"
    },
    "autoload": {
        "psr-0": {
            "Arara\\Process": "src"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Arara\\Test": "tests/src"
        }
    }
}
